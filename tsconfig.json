{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}